  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier": "prettier --write '**/*{.js,.jsx,.ts,.tsx}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
